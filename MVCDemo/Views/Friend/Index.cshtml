@{
}
@Model List <Friend>
    <div class="table">
        @foreach (Friend f in @Model)
        {
        <th>
        <td>@f.FriendId</td>
        </th>
        <th>
        <td>@f.FriendName</td>
        </th>
        <th>
        <td>@f.Place</td>
        </th>
        }
    </div>
